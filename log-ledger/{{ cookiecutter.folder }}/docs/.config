
#
# Document
#
CONFIG_DOC__TITLE="{{ cookiecutter.username }}'s {{ cookiecutter.thing }}"
CONFIG_DOC__AUTHOR="{{ cookiecutter.username }}"
CONFIG_DOC__YEAR={% now 'local', '%Y' %}
CONFIG_DOC__PROJECT="Personal notebook/diary"
CONFIG_DOC__LANGUAGE="de"
# CONFIG_DOC_LANGUAGE_ENGLISH is not set
CONFIG_DOC_LANGUAGE_GERMAN=y
CONFIG_DOC__CONFIDENTIALITY_LEVEL_LABEL="Vertraulichkeitsstufe"
CONFIG_DOC__CONFIDENTIALITY_LEVEL="Vertraulich"
# CONFIG_DOC__CONFIDENTIALITY_PUBLIC is not set
# CONFIG_DOC__CONFIDENTIALITY_INTERNAL is not set
CONFIG_DOC__CONFIDENTIALITY_CONFIDENTIAL=y
# CONFIG_DOC__CONFIDENTIALITY_STRICTLY_CONFIDENTIAL is not set
# end of Document

#
# CM
#
CONFIG_CM__TICKET_KEY=""
# end of CM

#
# SCM
#
CONFIG_SCM__HOST="bitbucket.vitronic.de:7998"
CONFIG_SCM__OWNER_KIND="users"
CONFIG_SCM__PROJECT=n
# CONFIG_SCM__USER is not set
CONFIG_SCM__OWNER="{{ cookiecutter.username }}"
CONFIG_SCM__REPO="{{ cookiecutter.thing }}"
CONFIG_SCM__REPO__URL_GIT_CLIENT="https://bitbucket.vitronic.de:7998/~{{ cookiecutter.username }}/{{ cookiecutter.username }}.git"
# end of SCM

#
# Build
#
CONFIG_BUILD__DIRS__SOURCE="docs"
CONFIG_BUILD__DIRS__CONFIG="docs"
CONFIG_BUILD__DIRS__BUILD="out/docs"
CONFIG_BUILD__PORTS__HTML__LIVE=1976
CONFIG_BUILD__PORTS__PRESENTATION__LIVE=1977
# end of Build

#
# Styling
#
CONFIG_STYLING__COLOR_PRIMARY=""
CONFIG_STYLING__COLOR_ACCENT=""
# end of Styling

#
# Publish
#
CONFIG_PUBLISH__HOST="docs.vitronic.de"
# end of Publish
