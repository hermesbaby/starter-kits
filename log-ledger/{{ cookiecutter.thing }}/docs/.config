
#
# Document
#
CONFIG_DOC__TITLE="{{ cookiecutter.username }}'s {{ cookiecutter.thing }}"
CONFIG_DOC__AUTHOR="{{ cookiecutter.username }}"
CONFIG_DOC__DESCRIPTION="Essays of an easy life taken seriously."
CONFIG_DOC__KEYWORDS="life, essay"
CONFIG_DOC__YEAR={% now 'local', '%Y' %}
CONFIG_DOC__PROJECT="{{ cookiecutter.thing }}"
CONFIG_DOC__LANGUAGE="en"
CONFIG_DOC_LANGUAGE_ENGLISH=y
# CONFIG_DOC_LANGUAGE_GERMAN is not set
CONFIG_DOC__CONFIDENTIALITY_LEVEL_LABEL="Confidentiality level"
CONFIG_DOC__CONFIDENTIALITY_LEVEL="Public"
CONFIG_DOC__CONFIDENTIALITY_PUBLIC=y
# CONFIG_DOC__CONFIDENTIALITY_INTERNAL is not set
# CONFIG_DOC__CONFIDENTIALITY_CONFIDENTIAL is not set
# CONFIG_DOC__CONFIDENTIALITY_STRICTLY_CONFIDENTIAL is not set
# end of Document

#
# CM
#
CONFIG_CM__TICKET_KEY=""
CONFIG_CM__URL_QUERY_TICKET="https://github.com/hermesbaby/starter-kits/issues/"
CONFIG_CM__URL_QUERY_USER="https://github.com/"
# end of CM

#
# SCM
#
CONFIG_SCM__HOST="github.com"
CONFIG_SCM__OWNER_KIND="users"
# CONFIG_SCM__PROJECT is not set
CONFIG_SCM__USER=y
CONFIG_SCM__OWNER="{{ cookiecutter.username }}"
CONFIG_SCM__REPO="{{ cookiecutter.thing }}"
CONFIG_SCM__REPO__URL_GIT_CLIENT="https://github.com/{{ cookiecutter.username }}/{{ cookiecutter.thing }}.git/"
# end of SCM

#
# Build
#
CONFIG_BUILD__DIRS__SOURCE="docs"
CONFIG_BUILD__DIRS__CONFIG="docs"
CONFIG_BUILD__DIRS__BUILD="out/docs"
CONFIG_BUILD__PORTS__HTML__LIVE=1976
CONFIG_BUILD__PORTS__PRESENTATION__LIVE=1973
# end of Build

#
# Styling
#
CONFIG_STYLING__COLOR_PRIMARY="grey"
CONFIG_STYLING__COLOR_ACCENT="amber"
# end of Styling

#
# Publish
#
CONFIG_PUBLISH__HOST="docs.some-host.com"
CONFIG_PUBLISH__BASEURL="https://www.some-host/some/path"
# end of Publish
